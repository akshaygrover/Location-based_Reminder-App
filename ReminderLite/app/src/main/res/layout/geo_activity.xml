<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />


    <ImageButton
        android:layout_width="@dimen/btn_wid"
        android:layout_height="@dimen/btn_hei"
        android:id="@+id/add_geofences_button"
        android:src="@drawable/btn_addgeo"
        android:scaleType="fitXY"
        android:layout_above="@+id/remove_geofences_button"
        android:layout_alignLeft="@+id/remove_geofences_button"
        android:layout_alignStart="@+id/remove_geofences_button"
        android:layout_marginBottom="20dp"
        />

    <ImageButton
        android:layout_width="@dimen/btn_wid"
        android:layout_height="@dimen/btn_hei"
        android:id="@+id/remove_geofences_button"
        android:src="@drawable/btn_remgeo"
        android:layout_above="@+id/textView9"
        android:layout_centerHorizontal="true"
        android:scaleType="fitXY" />

    <ImageButton
        android:layout_width="@dimen/btn_wid"
        android:layout_height="@dimen/btn_hei"
        android:id="@+id/viewgeo"
        android:src="@drawable/btn_viewgeo"
        android:scaleType="fitXY"
        android:layout_below="@+id/textView9"
        android:layout_centerHorizontal="true" />



    <ImageButton
        android:layout_width="@dimen/btn_wid"
        android:layout_height="@dimen/btn_hei"
        android:id="@+id/viewalarm"
        android:src="@drawable/btn_viewalrm"
        android:scaleType="fitXY"
        android:layout_below="@+id/viewgeo"
        android:layout_alignLeft="@+id/viewgeo"
        android:layout_alignStart="@+id/viewgeo"
        android:layout_marginTop="20dp"
       />
</RelativeLayout>